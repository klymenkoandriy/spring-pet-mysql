    <suppress files="[\\/]de[\\/]dlh[\\/]lht[\\/]cnote[\\/]service[\\/]archive[\\/]model[\\/].*" checks="StrictDuplicateCode"/>
    
    <suppress files=".*ArchiveIndexService\.java" checks=".*" />
    <suppress files=".*ArchiveIndexServiceBean\.java" checks=".*" />
    <suppress files=".*FullTextIndexDao\.java" checks=".*" />
    <suppress files=".*FullTextIndexDaoBean\.java" checks=".*" />
    <suppress files=".*FullTextIndexDaoBeanTestHelper\.java" checks=".*" />
